  "synopses": [
    {
      "synopsis": "The longest coast-to-coast sponsored series ever broadcast, 'Lets Dance' was a three-hour Saturday night musical program featuring 3 orchestras. An NBC orchestra led by Murray Kellner played sweet songs, Benny Goodman provided an hour of jazz, and Xavier Cugat provided and hour of Latin music. - (Handbook of Old-Time Radio p. 415)"
    }
  ],
  "themes": [
    {
      "theme": "Lets Dance, Goodbye, Lets' Have Another Cigarette"
    }
  ],
